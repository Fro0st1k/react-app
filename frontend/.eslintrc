{
  "parser":"babel-eslint",
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "react/prop-types": 0,
    "import/prefer-default-export": 0,
    "comma-dangle": ["error", "never"],
    "camelcase": 0,
    "react/destructuring-assignment": [true, "always", { "ignoreClassFields": true }]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
